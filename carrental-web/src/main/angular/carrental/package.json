{
  "name": "carrental",
  "version": "0.0.0",
  "license": "MIT",
  "angular-cli": {},
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config proxy.conf.json",
    "build": "ng build --prod --aot --bh /carrental-web/de/ --output-path dist/de --locale de --i18nFile=./src/locale/messages.xlf && ng build --prod --aot --bh /carrental-web/en/ --output-path dist/en --locale en --i18nFile=./src/locale/messages.en.xlf",
    "test": "ng test --browsers PhantomJS --reporters junit --single-run",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "i18n": "ng-xi18n -p src/tsconfig.json",
    "test-mvn": "ng test --browser PhantomJS --reporters junit --single-run"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.2.0",
    "@angular/common": "^5.2.0",
    "@angular/compiler": "^5.2.0",
    "@angular/core": "^5.2.0",
    "@angular/forms": "^5.2.0",
    "@angular/http": "^5.2.0",
    "@angular/platform-browser": "^5.2.0",
    "@angular/platform-browser-dynamic": "^5.2.0",
    "@angular/router": "^5.2.0",
    "core-js": "^2.4.1",
    "rxjs": "^5.5.6",
    "zone.js": "^0.8.19", 
    "bootstrap": "^3.2.6"           
  },
  "devDependencies": {
  	"@angular/cli": "1.7.0",    
    "@angular/compiler-cli": "^5.2.0",
    "@angular/language-service": "^5.2.0",   
    "@types/jasmine": "~2.8.3",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~6.0.60",
    "codelyzer": "^4.0.1",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~2.0.0",
    "karma-chrome-launcher": "~2.2.0",    
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.1.2",
    "ts-node": "~3.2.0",
    "tslint": "~5.9.1",
    "typescript": "~2.5.3",      	
    "karma-junit-reporter": "~1.2.0",
    "karma-phantomjs-launcher": "~1.0.4",
    "phantomjs-prebuilt": "~2.1.14"       
  }
}
